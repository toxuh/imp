{
  "name": "@toxuh/imp",
  "version": "1.0.2",
  "description": "Wrapper to run @toxuh/create-imp via npx",
  "type": "module",
  "bin": {
    "imp": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@toxuh/create-imp": "^1.0.2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run lint && vitest --run",
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build",
    "publish:pkg": "npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  },
  "keywords": [
    "nextjs",
    "next",
    "starter",
    "scaffold",
    "cli",
    "create-next-app",
    "shadcn",
    "tailwindcss",
    "prisma",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toxuh/imp.git",
    "directory": "packages/imp"
  },
  "homepage": "https://github.com/toxuh/imp#readme",
  "bugs": {
    "url": "https://github.com/toxuh/imp/issues"
  },
  "author": "Anton Zakharov <zakharov.toxa@gmail.com> (https://github.com/toxuh)",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
