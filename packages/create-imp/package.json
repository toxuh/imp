{
  "name": "@toxuh/create-imp",
  "version": "1.0.2",
  "description": "Next.js starter (Next 15 + shadcn + next-themes + optional Prisma).",
  "type": "module",
  "bin": {
    "create-imp": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run lint && vitest --run",
    "build": "tsup && node -e \"fs=require('fs');fs.cpSync('src/templates','dist/templates',{recursive:true})\"",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build",
    "publish:pkg": "npm publish --access public"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/prompts": "^2.4.9",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  },
  "keywords": [
    "nextjs",
    "next",
    "next.js",
    "create-next-app",
    "starter",
    "template",
    "boilerplate",
    "cli",
    "scaffold",
    "generator",
    "shadcn",
    "tailwindcss",
    "next-themes",
    "prisma",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toxuh/imp.git",
    "directory": "packages/create-imp"
  },
  "homepage": "https://github.com/toxuh/imp#readme",
  "bugs": {
    "url": "https://github.com/toxuh/imp/issues"
  },
  "author": "Anton Zakharov <zakharov.toxa@gmail.com> (https://github.com/toxuh)",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
