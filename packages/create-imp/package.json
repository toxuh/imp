{
  "name": "@toxuh/create-imp",
  "version": "0.1.0",
  "description": "Next.js starter (Next 15 + shadcn + next-themes + optional Prisma).",
  "type": "module",
  "bin": {
    "create-imp": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run lint && vitest --run",
    "build": "tsup && node -e \"fs=require('fs');fs.cpSync('src/templates','dist/templates',{recursive:true})\"",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build",
    "publish:pkg": "npm publish --access public"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/prompts": "^2.4.9",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  },
  "license": "MIT"
}
